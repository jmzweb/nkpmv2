*{
    margin:0;
    padding:0;
}
html
{
	height:100%;
	overflow:auto;
}
body{
    font-weight:normal;
    font-style:normal;
    font-family:宋体,Tahoma,Helvetica,Arial,sans-serif;
    font-size:14px;
    background-color:white;
    height:100%;
	text-align: center; 
	overflow:visible;
}
#maindiv
{
    width:800px;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	padding:20px 0px 20px 0px;
}
table.printtbl
{
    table-layout:fixed;
	border-collapse: collapse;
    margin-left:auto;
	margin-right:auto;
	font-size: 14px;
	border:1px #444 solid;
}
.printtbl caption{line-height:2px;font-size:20px;line-height:2;
                  font-family:"黑体","Microsoft YaHei UI"}
.printtbl td{
	text-align:left;
	padding:1px 1px 1px 3px;
	border:1px #444 solid;
	height:30px;
}
.printtbl td.noborder{
    border-width:0px;}

.noborder{
    border-width:0px;
}

.noborder td{
	border-width:0px;
}

table.noborder{
    border-collapse: collapse;
	
    }

table.subtbl
{
    table-layout:fixed;
	border-collapse: collapse;
    margin-left:auto;
	margin-right:auto;
	font-size: 14px;
}
.subtbl td {
	font-size: 14px;
	text-align:left;
	border:1px #444 solid;
	padding:1px 1px 1px 3px;
	height:25px;
}

.gray{color:Gray}
.red{color:red}
.green{color:green}
.hidden{display:none;}
.menubar
{
    
    line-height:34px;
    height:33px;
    overflow:hidden;
    z-index:100;
}
.topnav{
    width: 100%; 
    left:0px;
    display: block; 
    z-index: 100; 
    overflow: visible; 
    position: fixed; 
    top: 0px; 
    background-image: url(../img/toolbar_bg.gif);  
    height: 31px; 
    border-bottom:#cadcea 1px solid;
    font-size:12px;
}
a.linkbtn{
	line-height:20px;
	padding:0px;
	text-decoration:none;
	display:inline-block;
	color:Blue;
}
a.linkbtn:hover{
	text-decoration:underline;
	color:Red;
}
a.filelink{text-decoration:none;}
a.readlink{text-decoration:none;display:none;}
.split{
    color:#b1d7e9;
    width:10px;
    text-align:center;
    display:inline-block;
    font-style:normal;
    } 

.menubar ul{
	float: right; 
    list-style-type:none;
} 
.menubar ul li
{
    padding:0px 10px 0px 8px;
    float:left;
    margin-right:-2px;
    background:url(../img/edge1.gif) no-repeat 100% 50%
}
.menubar ul li a{
    text-decoration:none;
    color:Blue;
}
    
    
    
/*大表格样式*/
table.normaltbl,table.subtbl,table.dealInfoTbl
{
    table-layout:fixed;
	border-collapse: collapse;
    margin-left:auto;
	margin-right:auto;
	font-size: 12px;
	line-height:20px;
}

table.normaltbl
{
	border:#808080 1px solid;
	color:#393939;
}

.normaltbl caption
{
	font-weight:bold;
	font-size: 14pt;
	font-family: 黑体,宋体;
	color:#555;
    height:40px;
	line-height:40px;
	
}

.normaltbl > tr > td , .normaltbl > tbody > tr > td , .subtbl > tbody > tr > td, .subtbl > thead > tr > td
{
	text-align:left;
	padding:2px 2px 2px 3px;
	border:1px #808080 solid;
	line-height:24px;
	height:24px;
	word-wrap:break-word;
}


table.subtbl
{
    border:#808080 1px solid;
    color:#393939;
}
.subtbl > tbody > tr > td {
	border:1px #808080 solid;
	padding:2px 3px 2px 5px;
	height:24px;
	line-height:150%;
}
.subtbl > tbody > tr > td.DELBTN ,.subtbl > thead > tr > td.ADDBTN {
    text-align:center;
    height:24px;
    width:30px;
    padding-left:3px;
    text-decoration:none;
    }
    
.dealInfoTbl {
    border:1px #808080 solid;
    margin-top:15px;
    }
.dealInfoTbl td,.dealInfoTbl th{
    border:1px #808080 solid;
    padding:3px;
    line-height:24px;
	height:24px;}

input[type=radio],input[type=checkbox]
{
    vertical-align:middle;margin-right:3px;
}

.signspan{position:relative;height:40px;width:120px;display:inline-block;}
.signimg{position:absolute;top:-20px;left:0px;}
.center{margin-left:auto;margin-right:auto;}